@model IEnumerable<NanolekPrototype.EntityModels.Models.User>

<h2>Список пользователей</h2>
<table class="table">
    <tr><th>Email</th><th>ФИО</th><th>Должность</th><th></th></tr>
    @foreach (var user in Model)
    {
    <tr>
        <td>@user.Email</td>
        <td>@user.FullName </td>
        <td>@user.Position</td>
        <td>
            <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-userid="@user.Id">Права доступа</a>
        </td>
    </tr>
    }
</table>