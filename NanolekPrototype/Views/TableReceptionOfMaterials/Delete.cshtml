@model NanolekPrototype.EntityModels.Models.TableReceptionOfMaterial

@{
    ViewData["Title"] = "Delete";
}

<div class="header-and-back-button">
    <h1>Вы уверены, что хотите удалить это?</h1>
    <a class="btn nav-back-button" asp-controller="FormReceptionAndMovementOfPackingMaterials" asp-action="Details" asp-route-id="@Model.FormReceptionAndMovementOfPackingMaterialId">Вернуться назад</a>
</div>
<hr />
<div>
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Date)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Date)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.ReceivedFoil)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ReceivedFoil)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.RemainingProduction)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.RemainingProduction)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.SAPPart)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.SAPPart)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.ManufacturerSerialNumber)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ManufacturerSerialNumber)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.AnalyticalSheetNumberOKK)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.AnalyticalSheetNumberOKK)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.IsFoilMeetsControlParameters)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IsFoilMeetsControlParameters)
        </dd>
    </dl>

    <form asp-action="DeleteConfirmed">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Удалить" class="btn btn-danger" />
    </form>
</div>
